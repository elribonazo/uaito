{
  "name": "@uaito/electron",
  "description": "Analise packages",
  "version": "0.0.0-rc15",
  "private": true,
  "main": "main/index.js",
  "productName": "UAITO",
  "scripts": {
    "clean": "rimraf dist main renderer/out renderer/.next  ",
    "dev:ui": "NODE_ENV=development electron .",
    "dev:be": "NODE_ENV=development tsc -p electron-src",
    "build:renderer": "npx webpack -c ./renderer/webpack.prod.js",
    "build:electron": "tsc -p electron-src",
    "build": "npm run build:renderer && npm run build:electron",
    "watch:be": "tsc -p electron-src --watch",
    "dev": "concurrently  \"npm run dev:ui\" \"npm run watch:be\"",
    "type-check": "tsc -p ./renderer/tsconfig.json && tsc -p ./electron-src/tsconfig.json",
    "package": "electron-builder build -c release-config.js --mac --win --linux -c.extraMetadata.main=main/index.js",
    "package:mac": "electron-builder build -c release-config.js --mac -c.extraMetadata.main=main/index.js",
    "package:win": "electron-builder build -c release-config.js --win --x64 -c.extraMetadata.main=main/index.js",
    "package:linux": "electron-builder build -c release-config.js --linux -c.extraMetadata.main=main/index.js"
  },
